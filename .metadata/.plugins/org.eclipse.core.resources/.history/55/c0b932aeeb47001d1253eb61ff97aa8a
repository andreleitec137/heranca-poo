
public class NaviosDeGuerra extends Navio {
	
	private double blindagem;
	private double ataque;
	
	public NaviosDeGuerra(String n, int t, double b, double a) {
		super(n, t);
		blindagem = b;
		ataque = a;
	}
	
	public double getBlindagem() {
		return blindagem;
	}
	
	public double getAtaque() {
		return ataque;
	}
	
	public void poderDeFogo() {
		System.out.println("Poder de Fogo: " + getAtaque());
	}
	
